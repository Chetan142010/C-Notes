#include <iostream>
using namespace std;

int main() 
{
    // Integer data type used to store whole numbers
    int a = 10;
     
    //float data type used to store Decimal numbers
    float b = 50.6;
    double c = 10.6542325;
    char d = 'm';
    string e = "Messi";

    cout <<"Integer"<<a<<endl;
    cout <<"Float"<<b<<endl;
    
    return 0;
}